<div class="col-xs-12 page timeline-page">
	<div class="col-xs-6 hidden-xs">
		<h2 class="text-center">My Videos</h2>
	</div>
	<div class="col-xs-6 hidden-xs">
		<h2 class="text-center">Response</h2>
	</div>

	<ul class="timeline">
		<li>
			<div class="timeline-badge client"><i class="glyphicon glyphicon-user"></i></div>
			<div class="timeline-panel">
				<div class="timeline-heading">
					<h4 class="timeline-title"><span class="title-client">[Me]</h4>
					<p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> Posted 14 hours ago</small></p>
					<!--
					<div class="row">
						<div class="col-xs-12">
							<p class="client-description">Depressed</p>
						</div>
					</div>
					-->
					<div class="row">
						<div class="col-xs-12">
							<p></p>
						</div>
					</div>
				</div>
				<div class="timeline-body">
					<div class="video-wrap">
						<video class="video-video" id='MyPlayer1' data-uuid='[YOUR_VIDEO_UUID]'></video>
						<video class="video-video" src="https://s3.amazonaws.com/asynchealth/demo_patient.mp4"  controls autobuffer>
						</video>
					</div>
				</div>
			</div>
		</li>
		<li id="councillor-show" class="timeline-inverted">
			<div class="timeline-badge councillor"><i class="glyphicon glyphicon-user"></i></div>
			<div class="timeline-panel">
				<div class="timeline-heading">
					<h4 class="timeline-title"><span class="title-councillor">[Councillor]</span> Hao Yi</h4>
					<p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> Posted 11 hours ago</small></p>
				</div>
				<div class="timeline-body">
					<div class="video-wrap">
						<video class="video-video" src="https://s3.amazonaws.com/asynchealth/demo_counsellor.mp4"  controls autobuffer>
						</video>
					</div>
				</div>
			</div>
		</li>
	</ul>
	<div class="col-xs-12">
		<div class="col-xs-12 col-sm-2 col-sm-offset-5">
			<a id="respond-button" class="btn btn-lg btn-primary btn-block btn-respond">Respond</a>
		</div>
	</div>

	<div class="col-xs-12 well">
		<%= @timeline.inspect %>
	<% end %>

	<% if false %>
	<% if @timeline.videos.any? %>
	<ul class="timeline">
		<% @timeline.videos.each do |video| %>
		<li>
			<div class="timeline-badge client"><i class="glyphicon glyphicon-user"></i></div>
			<div class="timeline-panel">
				<div class="timeline-heading">
					<h4 class="timeline-title"><span class="title-client">[Me]</h4>
					<p><small class="text-muted"><i class="glyphicon glyphicon-time"></i> 11 hours ago via Twitter</small></p>
					<div class="row">
						<div class="col-xs-12">
							<p class="client-description"><%=video.question_id %></p>
						</div>
					</div>
					<div class="row">
						<div class="col-xs-12">
							<p></p>
						</div>
					</div>
				</div>
				<div class="timeline-body">
					<div class="video-wrap">
						<iframe class="video-video" width="560" height="315" src="<%= video.video_url %>" frameborder="0" allowfullscreen="allowfullscreen"></iframe>
					</div>
				</div>
			</div>
		</li>
	
		-->
		<% end %>
	</ul>
	<% else %>
		<!-- add message to start timeline -->
	<% end %>
	<% end #end false%>
</div>